{
  "name": "Liga-A-start-template",
  "version": "1.0.0",
  "description": "Стартовый шаблон сборки Лиги А",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && eslint source/js/",
    "build": "gulp build",
    "start": "gulp start",
    "webp": "gulp webp",
    "sprite": "gulp sprite",
    "deploy": "git push origin master && git subtree --prefix build push origin gh-pages",
    "server": "node server.js"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.15"
  },
  "dependencies": {
    "picturefill": "^3.0.3",
    "promise-polyfill": "8.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-yandex-maps": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^9.8.4",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "browser-sync": "^2.26.7",
    "core-js": "^3.6.5",
    "del": "^5.1.0",
    "eslint": "^7.4.0",
    "eslint-config-htmlacademy": "^0.5.2",
    "eslint-plugin-react": "^7.20.3",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-inject": "^5.0.5",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-webp": "^4.0.1",
    "pixel-glass": "^1.0.13",
    "posthtml-include": "^1.4.3",
    "react-svg-loader": "^3.0.3",
    "stylelint": "^13.6.1",
    "stylelint-config-htmlacademy": "^0.1.4",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^4.43.0",
    "webpack-stream": "^5.2.1"
  }
}
